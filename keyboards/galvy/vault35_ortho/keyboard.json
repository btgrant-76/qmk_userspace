{
    "manufacturer": "galvy",
    "keyboard_name": "vault35ortho",
    "maintainer": "haizeus",
    "url": "https://github.com/galvy0/vault35_ortho",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0xD106",
        "vid": "0x0411"
    },
    "bootloader": "atmel-dfu",
    "build": {
        "lto": true
    },
    "diode_direction": "COL2ROW",
    "features": {
        "audio": false,
        "backlight": false,
        "bootmagic": true,
        "command": false,
        "console": false,
        "encoder": true,
        "extrakey": true,
        "mousekey": false,
        "nkro": true,
        "rgb_matrix": false,
        "rgblight": false
    },
    "encoder": {
        "enabled": true,
        "rotary": [
            {"pin_a": "B5", "pin_b": "D1", "resolution": 4}
        ],
        "rotary_removed": [
            {"pin_a": "B6", "pin_b": "D0", "resolution": 4}
        ]
    },
    "matrix_pins": {
        "cols": ["F5", "F7", "D4", "B3", "B1", "C6", "B4", "D7", "D6", "D3", "D2"],
        "rows": ["F1", "B2", "F6", "F4"]
    },
    "processor": "atmega32u4",
    "qmk": {
        "locking": {
            "enabled": true,
            "resync": true
        },
        "tap_keycode_delay": 10
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [0, 0], "x": 0.0, "y": 0.0 },
                { "matrix": [0, 1], "x": 1.0, "y": 0.0 },
                { "matrix": [0, 2], "x": 2.0, "y": 0.0 },
                { "matrix": [0, 3], "x": 3.0, "y": 0.0 },
                { "matrix": [0, 4], "x": 4.0, "y": 0.0 },
                { "matrix": [0, 5], "x": 5.0, "y": 0.0 },
                { "matrix": [0, 6], "x": 6.0, "y": 0.0 },
                { "matrix": [0, 7], "x": 7.0, "y": 0.0 },
                { "matrix": [0, 8], "x": 8.0, "y": 0.0 },
                { "matrix": [0, 9], "x": 9.0, "y": 0.0 },
                { "matrix": [0, 10], "x": 10.0, "y": 0.0 },

                { "matrix": [1, 0], "x": 0.0, "y": 1.0 },
                { "matrix": [1, 1], "x": 1.0, "y": 1.0 },
                { "matrix": [1, 2], "x": 2.0, "y": 1.0 },
                { "matrix": [1, 3], "x": 3.0, "y": 1.0 },
                { "matrix": [1, 4], "x": 4.0, "y": 1.0 },
                { "matrix": [1, 5], "x": 5.0, "y": 1.0 },
                { "matrix": [1, 6], "x": 6.0, "y": 1.0 },
                { "matrix": [1, 7], "x": 7.0, "y": 1.0 },
